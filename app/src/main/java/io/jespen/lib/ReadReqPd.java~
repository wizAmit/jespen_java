package io.jespen.lib;

import com.eclipsesource.json.JsonObject;

public record ReadReqPd(int msg_id) implements ReqPayload {
    @Override
    public MsgType getMsgType() {
        return MsgType.read;
    }

    @Override
    public int getMsgId() {
        return msg_id();
    }

    @Override
    public JsonObject getJsonObject() {
        return null;
    }
}
